<application>
  <component name="ParameterNameHintsSettings">
    <blacklists>
      <blacklist language="PHP">
        <added pattern="\PHPUnit_Framework_TestCase.createMock(originalClassName)" />
        <added pattern="\Monolog\Logger.pushHandler(handler)" />
        <added pattern="\Symfony\Component\Serializer\SerializerInterface.serialize(data,format,context)" />
        <added pattern="\Eikona\Mammut\ConnectorBundle\Mammut.sendModificationNotification(location,payload)" />
        <added pattern="\Symfony\Bundle\FrameworkBundle\Controller\Controller.generateUrl(route,parameters,referenceType)" />
        <added pattern="\class_template.get_instance(str_template_name,str_abschnitt_name)" />
        <added pattern="\class_template.set_inhalt(str_platzhalter_name,str_wert)" />
        <added pattern="\class_database.get_result(str_sql,str_key)" />
      </blacklist>
    </blacklists>
  </component>
</application>